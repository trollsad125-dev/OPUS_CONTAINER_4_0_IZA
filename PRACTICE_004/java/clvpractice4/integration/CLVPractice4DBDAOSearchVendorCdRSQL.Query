<?xml version="1.0" encoding="UTF-8"?>
<sqls>
	<querys>
		<query name="CLVPractice4DBDAOSearchVendorCdRSQL">
			<desc><![CDATA[Vendor Code]]></desc>
			<sql><![CDATA[
SELECT VNDR_SEQ
FROM MDM_VENDOR A
WHERE 1 = 1
AND DELT_FLG = 'N'
AND A.VNDR_SEQ = @[vndr_seq]			]]></sql>
			<params>
				<param name="vndr_seq" type="12" value="" out="N"/>
			</params>
		</query>
	</querys>
</sqls>
